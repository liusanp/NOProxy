# 服务器配置
HOST=0.0.0.0
PORT=8000
DEBUG=true

# 访问密码
ACCESS_PASSWORD=your_password_here

# 目标网站配置 (请根据实际网站填写)
TARGET_BASE_URL=https://example.com
VIDEO_LIST_PATH=/videos

# Playwright配置
HEADLESS=true
BROWSER_TYPE=chromium

# 代理服务配置
PROXY_BASE_URL=http://localhost:8000

# 缓存配置
CACHE_ENABLED=true
CACHE_TTL=300

# 视频列表缓存有效期（秒），默认12小时
# VIDEO_LIST_CACHE_TTL=43200

# 已缓存视频列表每页数量，默认20
# CACHE_PAGE_SIZE=20
